{
  "name": "eslint-config-contactlab",
  "version": "4.2.0-0",
  "description": "Contactlab ESLint configuration",
  "main": "index.js",
  "author": "Contactlab",
  "license": "Apache-2.0",
  "repository": "contactlab/eslint-config-contactlab",
  "homepage": "https://github.com/contactlab/eslint-config-contactlab#readme",
  "bugs": "https://github.com/contactlab/eslint-config-contactlab/issues",
  "keywords": [
    "eslint",
    "eslintconfig",
    "tslint",
    "contactlab"
  ],
  "engines": {
    "node": ">= 10.18",
    "npm": ">= 6.13"
  },
  "files": [
    "index.js",
    "typescript.js"
  ],
  "scripts": {
    "pretest": "npm run build",
    "test:es": "eslint ./test/. -c ./test/_config.es.js --ext .js,.jsx",
    "test:ts": "eslint ./test/. -c ./test/_config.ts.js --ext .ts,.tsx",
    "test": "npm run test:es && npm run test:ts",
    "prebuild": "rm -f ./index.js ./typescript.js",
    "build": "tsc",
    "prepublishOnly": "npm test && npm run build"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint-plugin-import": "github:contactlab/eslint-plugin-import",
    "eslint-plugin-jsdoc": "^20.0.5"
  },
  "devDependencies": {
    "@types/eslint": "^6.1.3",
    "@types/node": "^12.12.24",
    "eslint": "^6.8.0",
    "husky": "^4.0.6",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
